/*---------------------------------------------
	process-pack #프로세스팩
---------------------------------------------*/
.process-pack {
  margin-top: 57px;
  padding: 0 10px;
  box-sizing: border-box;
  text-align: center;
}
.process-pack h4.se-title {
  margin-top: 17px;
}
.process-pack h4.se-title:first-child {
  margin-top: 0;
}
.process-pack .sub-msg {
  margin-top: 14px;
  font-size: 14px;
}
.process-pack .sub-msg.gry-01 {
  color: #707070;
}
.process-pack .sub-msg + .sub-msg {
  margin-top: 9px;
}
.process-pack .wait-time-box {
  margin-top: 30px;
}
.process-pack .error-msg {
  margin-top: 19px;
  font-size: 12px;
  color: #E42939;
}
.process-pack .progress-pack {
  max-width: 255px;
}

.progress-pack {
  margin: 30px auto 0;
}
.progress-pack .progress-fraction {
  margin-top: 7px;
  text-align: right;
  color: #000;
}

/*
	로그인
*/
.process-pack.ty-login .error-msg {
  min-height: 39px;
  margin-top: 6px;
}
.process-pack.ty-login .password-dots {
  height: 30px;
  margin: 45px 0 20px;
  padding-top: 14px;
  box-sizing: border-box;
}
.process-pack.ty-login .password-dots::after {
  content: "";
  display: inline-block;
  vertical-align: middle;
  width: 0;
  height: 100%;
}
.process-pack.ty-login .error-msg + .password-dots {
  margin-top: 0;
}
.process-pack.ty-login .btn-wrap {
  margin-top: 0;
  margin-bottom: 30px;
}
.process-pack.ty-login .login-img {
  margin-top: 80px;
}
.process-pack.ty-login .sub-msg + .btn-wrap .login-img {
  margin-top: 46px;
}
.process-pack.ty-login .cert-conts-pack {
  margin-top: 40px;
}

/* 로그인 방법 선택 */
.login-img {
  display: inline-block;
  width: 136px;
  height: 136px;
}
.login-img img {
  width: 100%;
}

.process-pack + .conts-part {
  margin-top: 36px;
}

/*
	대기시간
*/
.wait-time-box {
  display: inline-block;
  padding: 0 20px;
  box-sizing: border-box;
  width: 200px;
  height: 100px;
  line-height: 100px;
  border-radius: 12px;
  text-align: center;
  background-color: #F4F4F5;
}
.wait-time-box .time-set {
  display: inline-block;
  box-sizing: border-box;
  font-size: 50px;
  font-weight: 900;
  color: #1B6DFA;
}
.wait-time-box .time-set.end {
  color: #c8c8c8;
}

/*
	비밀번호
*/
.password-dots {
  width: 100%;
  height: 16px;
  text-align: center;
  font-size: 0;
}
.password-dots > span {
  display: inline-block;
  vertical-align: middle;
  width: 16px;
  height: 4px;
  border-radius: 8px;
  background-color: #E9EBEE;
}
.password-dots > span.on {
  height: 16px;
  background-color: #1B6DFA;
}
.password-dots > span + span {
  margin-left: 12px;
}

/*---------------------------------------------
	회원가입
---------------------------------------------*/
.border-rows-pack ul > li {
  height: 75px;
  padding-left: 15px;
  padding-right: 20px;
  border-bottom: 1px solid #E9EBEE;
  box-sizing: border-box;
}
.border-rows-pack ul > li .se-btn {
  display: flex;
  width: 100%;
  height: 100%;
  justify-content: space-between;
  align-items: center;
}
.border-rows-pack ul > li .se-btn span {
  font-size: 18px;
  line-height: 36px;
}

/*---------------------------------------------
	ocr 촬영
---------------------------------------------*/
.ocr-wrap {
  width: 100%;
  min-height: 460px;
  padding: 0 15px;
  box-sizing: border-box;
}
.ocr-wrap .camera-conts .ocr {
  overflow: hidden;
  width: 100%;
  height: 220px;
  border: 2px solid #fff;
  border-radius: 12px;
  box-sizing: border-box;
}
.ocr-wrap .camera-conts .ocr.focus {
  border: 4px solid #1B6DFA;
}
.ocr-wrap .camera-conts .cnt {
  margin-top: 20px;
  color: #fff;
  text-align: center;
}
.ocr-wrap .camera-conts .cnt .title {
  font-size: 21px;
  font-weight: bold;
}
.ocr-wrap .camera-conts .cnt .text {
  margin-top: 10px;
  font-size: 14px;
  color: #DDDDDD;
}
.ocr-wrap .info-conts {
  margin-top: 20%;
}
.ocr-wrap .info-conts ul {
  display: flex;
  justify-content: center;
  width: 100%;
}
.ocr-wrap .info-conts ul > li {
  text-align: center;
}
.ocr-wrap .info-conts ul > li .icon img {
  display: inline-block;
  width: 48px;
  height: 48px;
}
.ocr-wrap .info-conts ul > li .text {
  display: inline-block;
  margin-top: 10px;
  width: 100%;
  font-size: 14px;
  color: #DDDDDD;
}
.ocr-wrap .info-conts ul > li + li {
  margin-left: 30px;
}

/*---------------------------------------------
	앱 접근권한
---------------------------------------------*/
.app-access-wrap .access-conts .title .badge {
  display: inline-block;
  padding: 2px 8px;
  box-sizing: border-box;
  border-radius: 12px;
  font-size: 12px;
  font-weight: bold;
}
.app-access-wrap .access-conts .title .badge.clr-01 {
  background-color: #E9F8F8;
  color: #109C9F;
}
.app-access-wrap .access-conts .title .badge.clr-02 {
  background-color: #F4F4F5;
  color: #707070;
}
.app-access-wrap .access-conts .title + * {
  margin-top: 20px;
}
.app-access-wrap .access-conts ul.list {
  width: 100%;
}
.app-access-wrap .access-conts ul.list > li {
  display: flex;
  align-items: center;
  width: 100%;
}
.app-access-wrap .access-conts ul.list > li .cnt {
  margin-left: 12px;
}
.app-access-wrap .access-conts ul.list > li .cnt .tit {
  font-size: 14px;
  font-weight: bold;
}
.app-access-wrap .access-conts ul.list > li .cnt .tit + * {
  margin-top: 4px;
}
.app-access-wrap .access-conts ul.list > li .cnt .txt {
  font-size: 12px;
  color: #707070;
}
.app-access-wrap .access-conts ul.list > li + li {
  margin-top: 30px;
}
.app-access-wrap .access-conts + .access-conts {
  margin-top: 40px;
}

/*---------------------------------------------
	인증
---------------------------------------------*/
.cert-conts-pack ul.cert-list > li + li {
  margin-top: 10px;
}
.cert-conts-pack .noresult {
  margin-top: 60px;
}
.cert-conts-pack .noresult * {
  text-align: center;
}
.cert-conts-pack .noresult * + .text {
  margin-top: 20px;
}
.cert-conts-pack .noresult .text {
  font-size: 22px;
  font-weight: bold;
}

.cert-conts-pack.ty-edit ul.cert-list {
  margin-top: 20px;
}
.cert-conts-pack.ty-edit ul.cert-list > li + li {
  margin-top: 20px;
}

/* btn */
.btn-cert-info {
  overflow: hidden;
  display: flex;
  align-items: center;
  position: relative;
  padding: 25px 20px;
  border: 1px solid #E5E5E5;
  border-radius: 14px;
  background-color: #fff;
  font-size: 0;
}
.btn-cert-info.se-btn {
  width: 100%;
}
.btn-cert-info .info {
  width: calc(100% - 48px);
  padding-left: 16px;
  box-sizing: border-box;
}
.btn-cert-info .info .row {
  display: block;
  width: 100%;
  text-align: left;
}
.btn-cert-info .info .row * {
  display: inline-block;
  vertical-align: middle;
}
.btn-cert-info .info .row + .row {
  margin-top: 4px;
}
.btn-cert-info .info .name {
  font-size: 16px;
  font-weight: 900;
}
.btn-cert-info .info .status {
  font-size: 12px;
  margin-left: 6px;
}
.btn-cert-info .info .status.caution {
  color: #FF0000;
}
.btn-cert-info .info .text {
  font-size: 12px;
  color: #707070;
}
.btn-cert-info .info .text.side:before {
  content: "";
  display: inline-block;
  width: 1px;
  height: 10px;
  margin: 0 8px;
  background-color: #C8C8C8;
  vertical-align: middle;
}

/* 공동인증서 관리(버튼) */
.btn-cert-info.ty-edit {
  flex-wrap: wrap;
  padding-bottom: 0;
}
.btn-cert-info.ty-edit .btn-wrap {
  display: flex;
  width: calc(100% + 40px);
  margin: 20px -20px 0px;
  border-top: 1px solid #E5E5E5;
}
.btn-cert-info.ty-edit .btn-wrap > .se-btn {
  flex: 1;
  min-width: auto;
  border: none;
  border-radius: 0;
}
.btn-cert-info.ty-edit .btn-wrap > .se-btn + .se-btn {
  border-left: 1px solid #E5E5E5;
}

/*---------------------------------------------
	주소검색
---------------------------------------------*/
.search-address-wrap {
  margin-top: 20px;
}

.search-result-conts {
  margin-top: 40px;
}
.search-result-conts > .conts + ul.list {
  padding-top: 10px;
}
.search-result-conts > .conts .noresult {
  margin-top: 40px;
  text-align: center;
}
.search-result-conts > .conts .noresult * {
  text-align: center;
}
.search-result-conts > .conts .noresult * + .text {
  margin-top: 20px;
  font-size: 18px;
  font-weight: bold;
}
.search-result-conts > .conts > * + .row {
  margin-top: 10px;
}
.search-result-conts ul.list li {
  width: 100%;
  box-sizing: border-box;
  border-bottom: 1px solid #E9EBEE;
}
.search-result-conts ul.list li > a {
  width: 100%;
  height: 100%;
  padding: 18px 15px;
  box-sizing: border-box;
  text-align: left;
}
.search-result-conts ul.list .title {
  font-size: 16px;
  font-weight: bold;
}
.search-result-conts ul.list .conts {
  display: block;
  margin-top: 10px;
}
.search-result-conts ul.list .conts:first-child {
  margin-top: 0;
}
.search-result-conts ul.list .row {
  display: flex;
}
.search-result-conts ul.list .row .col-right {
  margin-left: auto;
  text-align: right;
  min-width: 155px;
}
.search-result-conts ul.list .row .se-badge {
  vertical-align: top;
}
.search-result-conts ul.list .row .se-badge + * {
  margin-left: 10px;
}
.search-result-conts ul.list .row .se-badge.size-fix-m {
  min-width: 68px;
}
.search-result-conts ul.list .row .se-badge.size-fix-l {
  min-width: 90px;
}
.search-result-conts ul.list .row .category {
  display: block;
  font-size: 16px;
  font-weight: 900;
}
.search-result-conts ul.list .row .category + .name {
  font-size: 15px;
  margin-top: 4px;
}
.search-result-conts ul.list .row .name {
  display: block;
  font-size: 16px;
  font-weight: bold;
  line-height: 1.2;
  color: #303030;
}
.search-result-conts ul.list .row .name + * {
  margin-top: 8px;
}
.search-result-conts ul.list .row .name + .detail {
  margin-top: 2px;
}
.search-result-conts ul.list .row .title {
  font-size: 16px;
  font-weight: bold;
  color: #303030;
}
.search-result-conts ul.list .row .text {
  font-size: 14px;
  font-weight: normal;
  color: #303030;
}
.search-result-conts ul.list .row .text.gry-01 {
  color: #707070;
}
.search-result-conts ul.list .row .date {
  font-size: 12px;
  color: #909090;
}
.search-result-conts ul.list .row .detail {
  display: block;
  font-size: 14px;
  color: #707070;
}
.search-result-conts ul.list .row .detail .val {
  display: inline-block;
  line-height: 1.2;
  vertical-align: middle;
}
.search-result-conts ul.list .row .detail .val.side:before {
  content: "·";
  display: inline-block;
  margin: 0 5px;
  vertical-align: middle;
}
.search-result-conts ul.list .row .detail + * {
  margin-top: 8px;
}
.search-result-conts ul.list .row .amount {
  display: block;
  line-height: 1.2;
  color: #1B6DFA;
}
.search-result-conts ul.list .row .amount .val {
  display: inline-block;
  font-size: 16px;
  font-weight: 900;
}
.search-result-conts ul.list .row .amount .unit {
  display: inline-block;
  margin-left: 2px;
  font-size: 12px;
  font-weight: bold;
}
.search-result-conts ul.list .row + .row {
  margin-top: 8px;
}
.search-result-conts .se-title .num {
  display: inline-block;
  margin: -4px 0 0 2px;
  font-size: 18px;
  font-weight: bold;
  color: #1B6DFA;
  vertical-align: middle;
}
.search-result-conts .se-title + .text {
  margin-top: 16px;
}
.search-result-conts .row {
  width: 100%;
  text-align: left;
}
.search-result-conts.nospace {
  margin-top: 0;
}
.search-result-conts.space-x {
  padding: 0 15px;
  box-sizing: border-box;
}
.search-result-conts.space-x ul.list {
  width: calc(100% + 30px);
  margin: 0 -15px;
}

/* ty-client */
.search-result-conts.ty-client ul.list .row {
  align-items: flex-start;
}
.search-result-conts.ty-client ul.list .row .se-badge {
  padding: 2.5px 8px;
  box-sizing: border-box;
  border-radius: 12px;
  font-size: 12px;
  font-weight: bold;
}
.search-result-conts.ty-client ul.list .row .se-badge.end {
  background-color: #F4F4F5;
  color: #707070;
}
.search-result-conts.ty-client ul.list .row .se-badge.ing {
  background-color: #E6F0FD;
  color: #1B6DFA;
}
.search-result-conts.ty-client ul.list .row .se-badge + .se-badge {
  margin-left: 4px;
}

.section .search-result-conts ul.list {
  width: calc(100% + 30px);
  margin: 0 -15px;
}

/* detail address */
.search-detail-wrap {
  margin-top: 20px;
}
.search-detail-wrap .detail-address {
  box-sizing: border-box;
}
.search-detail-wrap .detail-address .row {
  display: flex;
  width: 100%;
  text-align: left;
}
.search-detail-wrap .detail-address .row .se-badge {
  vertical-align: top;
}
.search-detail-wrap .detail-address .row .se-badge + * {
  margin-left: 10px;
}
.search-detail-wrap .detail-address .row .text {
  font-size: 14px;
  font-weight: normal;
  color: #303030;
}
.search-detail-wrap .detail-address .row .text.gry-01 {
  color: #707070;
}
.search-detail-wrap .detail-address .row + .row {
  margin-top: 8px;
}

/*---------------------------------------------
	차량검색
---------------------------------------------*/
/*
	car-result-box #차량겸색결과
*/
.car-result-box {
  margin-top: 20px;
  padding: 0 10px;
  box-sizing: border-box;
}
.car-result-box .car-info .conts {
  margin-top: 20px;
}
.car-result-box .car-info .conts .name {
  font-size: 22px;
  font-weight: 900;
}
.car-result-box .car-info .conts .sub-name {
  margin-top: 4px;
  font-size: 14px;
  font-weight: normal;
  color: #707070;
}
.car-result-box .car-info .info {
  display: flex;
  align-items: center;
  margin-top: 20px;
  padding-top: 20px;
  border-top: 1px solid #E9EBEE;
}
.car-result-box .car-info .info .title {
  font-size: 14px;
  color: #909090;
}
.car-result-box .car-info .info .title .se-tooltip {
  top: -2px;
}
.car-result-box .car-info .info .value {
  margin-left: auto;
  text-align: right;
}
.car-result-box .car-info .info .value * {
  display: inline-block;
  vertical-align: middle;
  line-height: 1.2;
}
.car-result-box .car-info .info .value .val {
  font-size: 22px;
  font-weight: 900;
}
.car-result-box .car-info .info .value .unit {
  margin-top: 6px;
  margin-left: 2px;
  font-size: 14px;
  font-weight: bold;
}
.car-result-box .swiper-pagination-fraction {
  display: inline-block;
  position: relative;
  bottom: auto;
  left: auto;
  width: auto;
  min-width: 44px;
  height: 24px;
  line-height: 24px;
  border-radius: 12px;
  margin-top: 10px;
  padding: 0 8px;
  box-sizing: border-box;
  font-size: 12px;
  color: #707070;
  background-color: #F4F4F5;
}
.car-result-box .swiper-pagination-fraction .swiper-pagination-current {
  font-weight: bold;
}
.car-result-box .swiper-pagination-fraction span {
  margin: 0 2px;
}

.section-jumbotron + .section .car-result-box {
  margin-top: 0;
}

