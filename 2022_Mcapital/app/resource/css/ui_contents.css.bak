/*
	.access-notice
*/
.access-notice {
  margin-top: 30px;
  padding-top: 29px;
  padding-left: 15px;
  padding-right: 15px;
  border-top: 1px solid #E9EBEE;
}
.access-notice .notice-sub .text {
  font-size: 12px;
  color: #707070;
}
.access-notice .notice-main {
  margin-top: 28px;
}

/*
	.ol-step
*/
ol.ol-step {
  counter-reset: step;
}
ol.ol-step li {
  position: relative;
  padding-left: 60px;
  box-sizing: border-box;
}
ol.ol-step li .img-wrap {
  position: absolute;
  top: 0;
  left: 0;
  width: 48px;
  height: 48px;
}
ol.ol-step li .img-wrap img {
  width: 100%;
}
ol.ol-step li .text {
  margin-top: 2px;
  font-size: 12px;
  color: #707070;
}
ol.ol-step > li:before {
  counter-increment: step;
  content: "Step " counter(step);
}
ol.ol-step > li + li {
  margin-top: 30px;
}

/*
	main-tdate-list #업무진행현황
*/
.main-tdate-list .tdate-list {
  display: flex;
  flex-wrap: wrap;
  font-size: 0;
  box-shadow: 0 -1px 0 0px #E9EBEE inset;
  box-sizing: border-box;
}
.main-tdate-list .tdate-list > li {
  width: 50%;
}
.main-tdate-list .tdate-list > li .conts {
  display: inline-flex;
  justify-content: space-between;
  position: relative;
  width: 100%;
  max-width: 100%;
  padding: 13px 10px 13px 15px;
  border-bottom: 1px solid #E9EBEE;
  box-sizing: border-box;
  text-align: left;
}
.main-tdate-list .tdate-list > li .conts .se-badge.ty-normal {
  flex: none;
  min-width: 26px;
}
.main-tdate-list .tdate-list > li .conts .title {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-right: 6px;
  font-size: 14px;
  font-weight: bold;
}
.main-tdate-list .tdate-list > li:nth-child(2n+1) .conts {
  padding-left: 0;
}
.main-tdate-list .tdate-list > li:nth-child(2n+1) .conts::before {
  content: "";
  position: absolute;
  top: 50%;
  right: -2px;
  transform: translateY(-50%);
  width: 1px;
  height: 18px;
  background-color: #E9EBEE;
}
.main-tdate-list .tdate-list > li:nth-child(2n) .conts {
  padding-right: 0;
}

/*
	sector-list #업종명리스트
*/
.sector-list {
  margin-top: 10px;
}
.sector-list .btn-sector {
  width: 100%;
  padding: 12px 0 15px;
  text-align: left;
  font-size: 16px;
}

/*
	app-guide-list #앱설명
*/
.app-guide-wrap {
  position: absolute;
  top: 55px;
  right: 0;
  left: 0;
  bottom: 0;
}

/* 앱설명 슬라이드 */
.app-guide-list {
  height: 100%;
  padding-bottom: 126px;
  box-sizing: border-box;
  text-align: center;
}
.app-guide-list .conts {
  padding: 0 24px;
  box-sizing: border-box;
}
.app-guide-list .conts .title {
  margin-top: 17px;
  font-size: 22px;
  font-weight: 900;
}
.app-guide-list .conts .text {
  margin-top: 11px;
  font-size: 16px;
  color: #707070;
}
.app-guide-list .conts .img {
  overflow: hidden;
  display: inline-block;
  max-width: 326px;
  height: 390px;
  margin-top: 20px;
  border-radius: 60px 60px 0 0;
  box-shadow: 0 -4px 10 rgba(0, 0, 0, 0.16);
}
.app-guide-list .conts .img img {
  max-width: 100%;
}
.app-guide-list .bottom {
  z-index: 1;
  position: absolute;
  right: 15px;
  bottom: 34px;
  left: 15px;
}
.app-guide-list .bottom .btn-wrap {
  margin-top: 24px;
}
.app-guide-list .bottom .btn-wrap .se-btn {
  display: none;
}
.app-guide-list .bottom .swiper-pagination {
  bottom: 74px;
  width: 100%;
}
.app-guide-list .swiper-pagination-bullet {
  opacity: 1;
  margin: 0 4px;
  width: 6px;
  height: 6px;
  border-radius: 50%;
  background-color: #E9EBEE;
  transition: width 0.2s;
}
.app-guide-list .swiper-pagination-bullet-active {
  width: 12px;
  border-radius: 3px;
  background-color: #366EF1;
}

.app-guide-list.last .bottom .btn-wrap .se-btn {
  display: block;
}

.app-guide-list.ty-ch .swiper-wrapper .swiper-slide:last-child .conts .img {
  margin-top: -7px;
}

@media (max-height: 667px) {
  .app-guide-list .conts .title {
    margin-top: 0;
  }
  .app-guide-list .conts .img {
    display: inline-block;
    height: 342px;
  }
  .app-guide-list .bottom .swiper-pagination {
    bottom: 64px;
  }
}
/*
	pdf-wrap #PDF박스
*/
.pdf-wrap {
  display: flex;
  flex-flow: column;
  border: 1px solid #E9EBEE;
  box-sizing: border-box;
  min-height: 200px;
  overflow-y: auto;
}
.pdf-wrap iframe {
  border: none;
  width: 100%;
  height: 100%;
}
.pdf-wrap img {
  width: 100%;
}
.pdf-wrap img + img {
  margin-top: 5px;
}

/*
	로딩 - 임시
*/
.loading_layer-temp {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(255, 255, 255, 0.8);
}

.loading_layer-temp .loading {
  position: absolute;
  top: 50%;
  left: 50%;
  margin: -8px 0 0 -14px;
  width: 28px;
  height: 16px;
}

.loading_layer-temp.allonebank .loading {
  width: 100px;
  height: 100px;
  margin: -50px 0 0 -50px;
}