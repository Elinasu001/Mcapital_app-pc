}

/*---------------------------------------------
	스플래쉬
---------------------------------------------*/
.splash-wrap {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  margin: auto;
  width: 100%;
  height: 100%;
  box-sizing: border-box;
}
.splash-wrap .splash-logo {
  margin-top: 186px;
  text-align: center;
}
.splash-wrap .splash-logo img {
  width: 90px;
}
.splash-wrap .splash-copy {
  margin-top: 68px;
  text-align: center;
  font-size: 16px;
  font-weight: bold;
}
.splash-wrap .splash-bottom {
  position: absolute;
  bottom: 54px;
  left: 50%;
  transform: translateX(-50%);
}
.splash-wrap .splash-bottom .version {
  font-size: 12px;
  color: #707070;
}

/*---------------------------------------------
	서류제출
---------------------------------------------*/
/*
	리스트
*/
.border-list > li .cont {
  padding: 23px 25px 25px 25px;
  border: 1px solid #E5E5E5;
  border-radius: 12px;
  box-sizing: border-box;
}
.border-list > li .cont h6 {
  width: 60%;
}
.border-list > li + li {
  margin-top: 10px;
}
.border-list > li.on .cont {
  border: 1px solid #1B6DFA;
}

/* upload-count */
.upload-count {
  display: flex;
  justify-content: space-between;
  margin-top: 10px;
}
.upload-count .count {
  font-size: 12px;
  color: #707070;
}
.upload-count .count span {
  color: #1B6DFA;
}
.upload-count .upload .se-btn + .se-btn {
  margin-left: 6px;
}

/* 썸네일리스트 */
.thumb-list {
  overflow-x: auto;
  overflow-y: hidden;
  margin-top: 10px;
  margin-right: -15px;
  margin-left: -15px;
  padding-bottom: 10px;
  box-sizing: border-box;
}
.thumb-list > ul {
  display: flex;
  padding-left: 15px;
  box-sizing: border-box;
}
.thumb-list > ul > li .se-btn {
  overflow: hidden;
  width: 100px;
  height: 100px;
  border-radius: 9px;
}
.thumb-list > ul > li .se-btn img {
  z-index: -1;
  position: relative;
  width: 100%;
  height: 100%;
}
.thumb-list > ul > li:last-child {
  padding-right: 15px;
  box-sizing: border-box;
}
.thumb-list > ul > li.last .se-btn {
