/*---------------------------------------------
	촬영서류확인 - swiper
---------------------------------------------*/
.chk-docs {
  position: relative;
  width: 100%;
}
.chk-docs > .inner {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.chk-docs > .inner > .top {
  position: relative;
  height: calc(100% - 134px);
  display: flex;
  align-items: center;
  justify-content: center;
}
.chk-docs > .inner > .top > img {
  width: 100%;
}
.chk-docs > .inner > .bottom {
  position: relative;
  width: 100%;
  height: 134px;
  background-color: #FFF;
  text-align: center;
  font-size: 0;
}
.chk-docs > .inner > .bottom > .left,
.chk-docs > .inner > .bottom > .center,
.chk-docs > .inner > .bottom > .right {
  position: absolute;
  top: 0;
  width: 33.3%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.chk-docs > .inner > .bottom > .left {
  left: 0;
}
.chk-docs > .inner > .bottom > .center {
  left: 50%;
  transform: translateX(-50%);
}
.chk-docs > .inner > .bottom > .right {
  right: 0;
}
.chk-docs > .inner > .bottom .hide {
  display: none;
}

.chk-docs.on > .inner > .top > img {
  width: auto;
  height: 100%;
}

.swiper-container.docs {
  width: 100%;
  height: 100%;
}
.swiper-container.docs > .se-btn {
  width: 36px;
  height: 36px;
  background-color: rgba(0, 0, 0, 0.2);
  background-repeat: no-repeat;
  background-position-y: center;
  background-size: 12px 14px;
  border-radius: 50px;
}
.swiper-container.docs > .se-btn:after {
  content: none;
}
.swiper-container.docs > .se-btn.swiper-button-prev {
  background-position-x: 9px;
  background-image: url("../image/icon/icon_pagi_prev.png");
}
.swiper-container.docs > .se-btn.swiper-button-next {
