/*---------------------------------------------
	progress-bar #프로그레스바
---------------------------------------------*/
.progress-bar {
  overflow: hidden;
  position: relative;
  width: 100%;
  height: 7px;
  border-radius: 7px;
  background-color: #E9EBEE;
}
.progress-bar .bar {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 7px;
  border-radius: 7px;
  background-color: #1B6DFA;
}
.progress-bar.ptn-01 {
  background-color: #E9EBEE;
}
.progress-bar.ptn-01 .bar {
  background-image: url("../image/contents/bg_progress_clr01.png");
  background-size: auto 7px;
  background-repeat: repeat-x;
}
.progress-bar.ptn-02 {
  background-color: #E9EBEE;
}
.progress-bar.ptn-02 .bar {
  background-image: url("../image/contents/bg_progress_clr02.png");
  background-size: auto 7px;
  background-repeat: repeat-x;
}
.progress-bar.clr-01 {
  background-color: #000;
}
.progress-bar.clr-01 .bar {
  background-color: #FFF;
}
.progress-bar.clr-02 {
  background-color: #E9F8F8;
}
.progress-bar.clr-02 .bar {
  background-color: #2DB9BC;
}

/* 서류제출 */
.progress-bar.ty-doc {
  height: 12px;
  border-radius: 6px;
  border: 1px solid #E9EBEE;
  padding: 1px;
  box-sizing: border-box;
  background-color: #F8F8F8;
}
.progress-bar.ty-doc .bar {
  position: relative;
  top: auto;
  left: auto;
  width: 100%;
  height: 8px;
  border-radius: 4px;
  background-color: #1B6DFA;
}

