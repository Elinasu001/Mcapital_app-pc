/*---------------------------------------------
	text field #텍스트필드
---------------------------------------------*/
/*
	reset
*/
.se-textfld {
  position: relative;
  display: inline-block;
  width: 100%;
  box-sizing: border-box;
}

.se-textfld input {
  display: inline-block;
  width: 100%;
  height: 100%;
  -webkit-appearance: none;
  -moz-appearance: none;
  appearance: none;
  border-radius: 0;
  border: none;
  outline: none;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
}

.se-textfld input:focus {
