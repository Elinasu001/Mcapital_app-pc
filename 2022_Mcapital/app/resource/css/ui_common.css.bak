/*---------------------------------------------
	loader #로딩
---------------------------------------------*/
.loader {
  position: relative;
  margin: 20px auto 0;
  width: 36px;
}
.loader:before {
  content: "";
  display: block;
  padding-top: 100%;
}

.circular {
  animation: rotate 2s linear infinite;
  height: 100%;
  transform-origin: center center;
  width: 100%;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
}

.path {
  stroke: #c8c8c8;
  stroke-dasharray: 1, 200;
  stroke-dashoffset: 0;
  stroke-linecap: round;
  animation: dash 1.5s ease-in-out infinite;
}

@keyframes rotate {
  100% {
    transform: rotate(360deg);
  }
}
@keyframes dash {
  0% {
    stroke-dasharray: 1, 200;
    stroke-dashoffset: 0;
  }
  50% {
    stroke-dasharray: 89, 200;
    stroke-dashoffset: -35px;
  }
  100% {
    stroke-dasharray: 89, 200;
    stroke-dashoffset: -124px;
  }
}
/*---------------------------------------------
	se-chart #도넛차트#차트
---------------------------------------------*/
.se-chart {
  position: relative;
  width: 132px;
  height: 132px;
  transform: rotate(-90deg);
}
.se-chart .center {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 70px;
  height: 70px;
  border-radius: 50%;
  background-color: #fff;
}
.se-chart .circle {
  transform-origin: center;
}
.se-chart .circle.ani.clr-02 {
  stroke: #1B6DFA;
}
.se-chart .circle.ani.clr-03 {
  stroke: #2DB9BC;
}

.se-chart-legend > ul > li + li {
  margin-top: 11px;
}
.se-chart-legend .se-btn {
  display: flex;
  align-items: center;
}
.se-chart-legend .title {
  position: relative;
  padding-left: 15px;
  color: #303030;
}
.se-chart-legend .title::before {
  content: "";
  position: absolute;
  left: 0;
  top: 50%;
  transform: translate(0, -50%);
  width: 7px;
  height: 7px;
  border-radius: 50%;
}
.se-chart-legend .title.clr-02::before {
  background-color: #1B6DFA;
}
.se-chart-legend .title.clr-03::before {
  background-color: #2DB9BC;
}
