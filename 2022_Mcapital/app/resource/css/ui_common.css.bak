/*---------------------------------------------
	Button #버튼
---------------------------------------------*/
/*
	reset
*/
.se-btn {
  -webkit-appearance: none;
  appearance: none;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  text-decoration: none;
  border: 1px solid transparent;
  cursor: pointer;
}

.se-btn[disabled=disabled],
.se-btn:disabled,
.se-btn.disabled {
  cursor: default;
}

.se-btn {
  display: inline-block;
  padding: 0;
  background: transparent;
  text-align: center;
  vertical-align: middle;
  font-weight: normal;
}

/*
	default
*/
.se-btn {
  color: #303030;
  font-size: 14px;
}

/* btn-solid */
.se-btn.btn-solid {
  min-width: 100%;
  min-height: 42px;
  padding: 10px;
  border-radius: 12px;
  background-color: #F4F4F5;
  color: #707070;
}
.se-btn.btn-solid[disabled=disabled], .se-btn.btn-solid:disabled, .se-btn.btn-solid.disabled {
  background-color: #E9EBEE;
  color: #C8C8C8;
}
.se-btn.btn-solid:active:not(:disabled) {
  background-color: #E9EBEE;
  color: #707070;
}
.se-btn.btn-solid.size-xs {
  padding: 4px 9px;
  min-width: 46px;
  min-height: 28px;
  border-radius: 9px;
  font-weight: normal;
}
.se-btn.btn-solid.size-s {
  min-width: 90px;
}

/* btn-solid-line */
.se-btn.btn-solid-line {
  min-width: 100%;
  min-height: 42px;
  padding: 10px;
  border-radius: 12px;
  border: 1px solid #E9EBEE;
  background-color: #fff;
  color: #707070;
}
.se-btn.btn-solid-line[disabled=disabled], .se-btn.btn-solid-line:disabled, .se-btn.btn-solid-line.disabled {
  background-color: #E9EBEE;
  border-color: #E3E5E8;
  color: #C8C8C8;
}
.se-btn.btn-solid-line:active:not(:disabled) {
  background-color: #F4F4F5;
  border-color: #E3E5E8;
}
.se-btn.btn-solid-line.size-s {
  min-width: 90px;
}

/* btn-plain */
.se-btn.btn-plain {
  min-width: 100%;
  min-height: 42px;
  padding: 10px;
  border-radius: 12px;
  background-color: transparent;
  color: #1B6DFA;
}
.se-btn.btn-plain[disabled=disabled], .se-btn.btn-plain:disabled, .se-btn.btn-plain.disabled {
  background-color: transparent;
  color: #C8C8C8;
}
.se-btn.btn-plain:active:not(:disabled) {
  background-color: #E6F0FD;
  color: #1B6DFA;
}
.se-btn.btn-plain.size-s {
