  border-color: #E3E5E8;
  color: #C8C8C8;
}
.se-btn.btn-stroke:active:not(:disabled) {
  background-color: #F4F4F5;
  border-color: #E3E5E8;
}

/*---------------------------------------------
	Button Set #버튼셋 #버튼세트
---------------------------------------------*/
.sp-btnset {
  display: flex;
  width: 100%;
}
.sp-btnset .se-btn {
  min-width: auto;
  width: 100%;
  border-radius: 0;
}
.sp-btnset .se-btn:first-child {
  border-radius: 12px 0 0 12px;
}
.sp-btnset .se-btn:last-child {
  border-radius: 0 12px 12px 0;
}
.sp-btnset .se-btn.btn-solid-line + .btn-solid-line {
  border-left-color: transparent;
}
.sp-btnset .se-btn.btn-solid-point:disabled {
  border-left-color: transparent;
  border-right-color: transparent;
}
.sp-btnset .se-btn.btn-solid-point:disabled:first-child {
  border-left-color: #E3E5E8;
}
.sp-btnset .se-btn.btn-solid-point:disabled:last-child {
  border-right-color: #E3E5E8;
}
.sp-btnset .se-btn.btn-solid-point:disabled + .btn-solid-point:before {
  background: #E3E5E8;
}
.sp-btnset .se-btn.btn-solid-point + .btn-solid-point {
  position: relative;
}
.sp-btnset .se-btn.btn-solid-point + .btn-solid-point:before {
  content: "";
  position: absolute;
  top: 50%;
  left: 0;
  width: 1px;
  height: 22px;
  margin-top: -11px;
  background: #4B8DFF;
}

