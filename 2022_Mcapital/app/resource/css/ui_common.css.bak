/*---------------------------------------------
	accordion #아코디언
---------------------------------------------*/
.sp-accord {
  width: 100%;
}
.sp-accord > li {
  position: relative;
}
.sp-accord > li::before {
  content: "";
  display: inline-block;
  position: absolute;
  bottom: 0;
  left: 50%;
  transform: translate(-50%, 0);
  width: 100%;
  height: 1px;
  background-color: #E9EBEE;
}
.sp-accord > li .p-accord-title {
  position: relative;
  width: 100%;
  padding: 10px 55px 10px 0;
  box-sizing: border-box;
}
.sp-accord > li .p-accord-conts {
  display: none;
  width: 100%;
  background-color: #ececec;
}
.sp-accord > li .p-accord-conts > .conts {
  padding: 10px 0;
  box-sizing: border-box;
}
.sp-accord > li .se-btn.btn-accod-open {
  position: absolute;
  right: 0;
  top: 50%;
  margin-top: -20px;
  width: 40px;
  height: 40px;
}
.sp-accord > li.on::before {
  content: none;
}
.sp-accord > li.on .se-btn.btn-accod-open {
  transform: rotate(180deg);
}
.sp-accord > li.on .p-accord-conts {
  display: block;
}

/* ty-faq */
.sp-accord.ty-faq > li::before {
  transform: translate(-50%, 0);
  width: calc(100% - 30px);
}
.sp-accord.ty-faq > li .p-accord-title {
  position: relative;
  padding: 19px 48px 20px 39px;
}
.sp-accord.ty-faq > li .p-accord-title::before {
  content: "Q";
  position: absolute;
  top: 16px;
  left: 15px;
  font-size: 16px;
  font-weight: 900;
}
.sp-accord.ty-faq > li .p-accord-title .title {
  font-weight: bold;
}
.sp-accord.ty-faq > li .p-accord-conts {
  display: none;
  position: relative;
  width: 100%;
  padding: 18px 18px 20px 39px;
  box-sizing: border-box;
  background-color: #F8F8F8;
}
.sp-accord.ty-faq > li .p-accord-conts::before {
  content: "A";
  position: absolute;
  top: 16px;
  left: 16px;
  font-size: 17px;
  font-weight: 900;
  color: #1B6DFA;
}
.sp-accord.ty-faq > li .p-accord-conts .conts {
  padding: 0;
  color: #707070;
}
.sp-accord.ty-faq > li .se-btn.btn-accod-open {
  right: 4px;
}
.sp-accord.ty-faq > li.on::before {
  display: none;
}
.sp-accord.ty-faq > li.on .p-accord-conts {
  display: block;
}

.se-ico.ico-accord-open {
  width: 18px;
  height: 18px;
  background-image: url("../image/icon/icon_accord_open.png");
}

.se-ico.ico-faq-open {
  width: 18px;
  height: 18px;
  background-image: url("../image/icon/icon_accord_open.png");
}

