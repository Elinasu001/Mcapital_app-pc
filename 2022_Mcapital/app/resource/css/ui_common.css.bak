*/
/* default */
.sp-bullet-list > li {
  position: relative;
  padding-left: 10px;
  font-size: 12px;
  color: #909090;
}
.sp-bullet-list > li:before {
  content: "";
  position: absolute;
  left: 0;
  top: 0.7em;
  color: inherit;
}
