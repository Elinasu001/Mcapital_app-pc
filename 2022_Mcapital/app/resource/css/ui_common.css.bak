/*---------------------------------------------
	range #레인지
---------------------------------------------*/
.se-range.noUi-target {
  width: 100%;
  height: 22px;
  border: none;
  box-shadow: none;
  background-color: transparent;
}
.se-range .noUi-base {
  position: relative;
  width: calc(100% - 22px);
  height: 100%;
  margin: 0 auto;
  font-size: 0;
}
.se-range .noUi-base:before {
  content: "";
  display: inline-block;
  vertical-align: middle;
  width: 0;
  height: 100%;
}
.se-range .noUi-base:after {
  content: "";
  z-index: -1;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  left: -11px;
  width: calc(100% + 22px);
  height: 4px;
  border-radius: 4px;
  background-color: #E9EBEE;
}
.se-range .noUi-connects {
  overflow: visible;
  display: inline-block;
  vertical-align: middle;
  width: 100%;
  height: 4px;
}
.se-range .noUi-connect {
