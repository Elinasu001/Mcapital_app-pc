}

.se-btn.btn-link-box.ty-bg {
  padding: 24px 40px 24px 25px;
  background-color: #F8F8F8;
  border: 1px solid #F8F8F8;
}

/* btn-stroke */
.se-btn.btn-stroke {
  min-height: 32px;
  padding: 0 14px;
  border-radius: 12px;
  border: 1px solid #E9EBEE;
  color: #707070;
